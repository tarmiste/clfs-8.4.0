    xpointer="xpointer(//*[@os='a'])"/>

<screen os="b"><userinput remap="configure">CC="gcc ${BUILD32}" ./config --prefix=/usr         \
         --openssldir=/etc/ssl \
         --libdir=lib32        \
         shared                \
         zlib-dynamic </userinput></screen>

